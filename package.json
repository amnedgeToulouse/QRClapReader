{
  "build": {
    "appId": "qrclap.reader",
    "productName": "QRClap Reader",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "QRClap Reader",
      "uninstallDisplayName": "QRClap Reader Uninstall"
    },
    "files": [
      "**/*",
      "!.github",
      "!.vscode",
      "!.code-workspace",
      "!.npmrc",
      "!*.md",
      "!package.json",
      "!package-lock.json",
      "!e2e/",
      "!hooks/",
      "!_config.yml",
      "!angular.json",
      "!tsconfig*.json",
      "!tslint.json",
      "!.eslintrc.json",
      "!karma.conf.js",
      "!angular.webpack.js",
      "!*.map",
      "!dist/win-unpacked",
      "!save-param.json",
      "dist/**"
    ],
    "fileAssociations": [
      {
        "name": "QRClapBackup",
        "description": "QRClap file to do a backup rename",
        "ext": [
          "qrc"
        ]
      }
    ]
  },
  "name": "QRClapReader",
  "version": "1.0.10",
  "description": "QRClap Reader allow you to read QR generated from QRClap generator mobile app",
  "homepage": "https://qrclap.com",
  "author": {
    "name": "Gabriel GODARD",
    "email": "contact.qrclap@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amnedgeToulouse/QRClapReader.git"
  },
  "keywords": [
    "angular",
    "angular 11",
    "electron",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:deploy": "npm run build:prod && electron-builder build --publish=always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/cdk": "11.2.12",
    "@angular/material": "11.2.12",
    "@electron/remote": "1.0.4",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ng-select/ng-select": "6.1.0",
    "angular-animations": "0.11.0",
    "angular-font-awesome": "3.1.2",
    "app-root-dir": "1.0.2",
    "bootstrap": "4.6.0",
    "bootstrap4-toggle": "3.6.1",
    "copy-dir": "1.3.0",
    "cpy": "8.1.2",
    "electron-root-path": "1.0.16",
    "electron-updater": "4.3.9",
    "ffmpeg-static": "4.3.0",
    "font-awesome": "4.7.0",
    "fs-extra": "10.0.0",
    "get-video-duration": "3.0.2",
    "http": "0.0.1-security",
    "https": "1.0.0",
    "jquery": "3.6.0",
    "jwt-decode": "3.1.2",
    "md5-file": "5.0.0",
    "ncp": "2.0.0",
    "ngx-electron": "2.2.0",
    "node-recursive-directory": "1.2.0",
    "simple-thumbnail": "1.6.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.7",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "11.2.7",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/compiler-cli": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/language-service": "11.2.8",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.6.7",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.15.0",
    "@typescript-eslint/parser": "4.16.1",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.3",
    "electron": "12.0.2",
    "electron-builder": "22.10.5",
    "electron-reload": "1.5.0",
    "eslint": "7.22.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.6",
    "spectron": "14.0.0",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.0.5",
    "wait-on": "5.0.1",
    "webdriver-manager": "12.1.8",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
